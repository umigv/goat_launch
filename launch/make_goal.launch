<launch>

	<arg name="goal_id_in" default=""/>
	<arg name="filename" default="test.txt"/>
	<arg name="threshold_in" default="0.5"/>

	<node name="make_goal" pkg="make_goal" type="make_goal" output="screen" clear_params="true">

		<param name="goal_id" value="$(arg goal_id_in)"/>
		<param name="goals_filename" value="$(find goat_launch)/params/$(arg filename)"/>
		<param name="threshold" value="$(arg threshold_in)"/>

	</node>

</launch>
